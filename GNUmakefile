.POSIX:
.SUFFIXES:
.PHONY: all clean install check
all:
PROJECT=go-i18n-error
VERSION=1.0.0
PREFIX=/usr/local

## -- BLOCK:go --
## -- BLOCK:go --
## -- BLOCK:license --
install: install-license
install-license: 
	mkdir -p $(DESTDIR)$(PREFIX)/share/doc/$(PROJECT)
	cp LICENSE $(DESTDIR)$(PREFIX)/share/doc/$(PROJECT)
